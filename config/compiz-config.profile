[ezoom]
s0_zoom_box_outline_color = #2f2f4f9f
s0_zoom_box_fill_color = #2f2f2f4f

[wall]
s0_thumb_highlight_gradient_shadow_color = #dfdfdfff
s0_arrow_base_color = #e6e6e6d9
s0_arrow_shadow_color = #dcdcdcd9

[expo]
s0_ground_color1 = #b3b3b3cc
s0_ground_color2 = #b3b3b300

[animation]
s0_unminimize_effects = animation:Glide 2;

[switcher]
s0_speed = 9,400000
s0_timestep = 0,100000
s0_mipmap = false
s0_brightness = 0
s0_opacity = 0
s0_bring_to_front = false
s0_icon = false

[commands]
s0_command0 = notify-send "`date \"+%A %d %B, %T\"`"
s0_run_command0_key = <Control>oe

[grid]
s0_cycle_sizes = true

[gnomecompat]
s0_main_menu_key = <Super>s
s0_command_window_screenshot = gnome-screenshot -w
s0_command_terminal = x-terminal-emulator
s0_run_command_terminal_key = <Super>t

[obs]
s0_opacity_increase_button = Disabled
s0_opacity_decrease_button = Disabled
s0_brightness_increase_button = <Alt>Button4
s0_brightness_decrease_button = <Alt>Button5

[unityshell]
s0_inactive_shadow_color = #000000a5
s0_alt_tab_forward = Disabled
s0_alt_tab_prev = Disabled

[water]
s0_initiate_key = <Control><Super>F9

[decor]
s0_active_shadow_color = #00000080
s0_inactive_shadow_color = #000000ff
s0_decoration_match = any & !class=Firefox & !class=X-terminal-emulator & !class=Sublime_text & !class=Chromium-browser

[freewins]
s0_snap_threshold = 50
s0_circle_color = #54befb80
s0_line_color = #1800ffff
s0_cross_line_color = #1800ffff

[scale]
s0_spacing = 0
s0_button_bindings_toggle = true
s0_initiate_edge = BottomRight

[rotate]
s0_flip_time = 349
s0_sensitivity = 2,810000
s0_speed = 7,000000
s0_timestep = 0,500000

[core]
s0_active_plugins = core;composite;opengl;compiztoolbox;decor;place;neg;regex;imgpng;resize;snap;vpswitch;screenshot;shift;animation;workarounds;commands;grid;gnomecompat;mousepoll;move;obs;fade;session;cube;rotate;expo;unitymtgrabhandles;scale;ezoom;scaleaddon;switcher;
s0_outputs = 1920x1080+0+0;1920x1080+1920+0;
s0_hsize = 4

[resizeinfo]
s0_gradient_1 = #cccce6cc
s0_gradient_2 = #f3f3f3cc
s0_gradient_3 = #d9d9d9cc
s0_outline_color = #e6e6e6ff

[showmouse]
